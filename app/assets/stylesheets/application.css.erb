/*
 * This is a manifest file that'll be compiled into application.css, which will include all the files
 * listed below.
 *
 * Any CSS and SCSS file within this directory, lib/assets/stylesheets, or any plugin's
 * vendor/assets/stylesheets directory can be referenced here using a relative path.
 *
 * You're free to add application-wide styles to this file and they'll appear at the bottom of the
 * compiled file so the styles you add here take precedence over styles defined in any other CSS/SCSS
 * files in this directory. Styles in this file should be added after the last require_* statement.
 * It is generally better to create a new file per style scope.
 *
 *= require_tree .
 *= require_self
 */
html {
  background-color: #c0c5dc;
  display: flex;
  align-items: center;
  justify-content: center;
}

.tict {
  width: 100%;
}

.tict button {
  width: 300px;
  background-color: #5b8e1f;
  border: 1px solid gray;
  padding: 5px;
  border-radius: 5px;
  font-size: 20px;
}

body {
  width: 100%;
  height: 100%;
  margin: 0 auto;
  color: black;
  font-family: 'Roboto Condensed', sans-serif;
}

.content {
  margin: 5px;
  margin-top: 3em;
}

h1 {
  font-family: 'Sedgwick Ave Display', cursive;
  color: black;
  font-weight: normal;
  margin: 2px auto;
  text-align: center;
}

.refresh {
  color: black;
}

.winner {
  font-size: 18px;
}

.headertext {
  color: white;
  margin: 0;
  margin-top: -8px;
  font-size: 22px;
}

.notecount {
  color: #040e23;
  border-radius: 50%;
  background-color: white;
  padding: 2px;
}

h2, h3 {
  font-family: 'Roboto Condensed', sans-serif;
  color: black;
  margin: 2px auto;
  text-align: center;
}

a {
  text-decoration: none;
  font-weight: bold;
  color: white;
}
form {
  display: inline;
}
input, textarea {
  border-radius: 3px;
  padding: 3px 5px;
  border: 1px solid gray;
  margin: 3px;
  font-family: 'Roboto Condensed', sans-serif;
  width: 90%;
}
input[type='submit'] {
  width:auto;
}
select {
  background-color: white;
  display: inline;
  border-radius: 3px;
  padding: 3px 5px;
  border: 1px solid gray;
  margin: 3px;
  font-family: 'Roboto Condensed', sans-serif;
}
textarea {
  width: 90%;
  height: 3em;
}

header {
  z-index: 255;
  top: 0;
  width: 100%;
  height: 40px;
  max-height: 40px;
  margin: 0 auto;
  position: fixed;
  overflow: hidden;
  border-bottom: 1px solid gray;
  color: white;
  background-color: #040e23;
  background-image: url('<%= image_path('nightjar.png') %>');
  background-position: left;
  background-repeat: no-repeat;
  background-size: contain;
  font-family: 'Roboto Condensed', sans-serif;
}

.points {
  font-weight: bold;
  color: #caa741;
  font-size: 18px;
}

#slotmachine {
  text-align: center;
  margin: 0 auto;
}

#slotmachine button {
  width: 400px;
  height: 100px;
  font-size: 50px;
  text-align: center;
  background-color: #94c15e;
  border: none;
}

.big {
  font-size: 30px;
}
.notebody {
  border-left: 1px solid gray;
  margin-left: 1em;
  padding-left: .5em;
}
.new_notification input[type='number']{
  width: 100px;
}
#slotreel, #slotflash {
  width: 80%;
  height: 100px;
  background-color: #d2d2d2;
  margin: 0 auto;
  padding: 1em 0;
  display: grid;
  grid-template-columns: repeat(3, 30%);
  grid-gap: 5px;
  padding: 5px;
}

#slotflash {
  height: 2em;
}

#slotreel h3 {
  font-size: 50px;
  font-family: 'Roboto Condensed', sans-serif;
}

#slotreel p {
  font-weight: normal;
  color: black;
  font-family: 'Roboto Condensed', sans-serif;
}

.reel {
  width: 100%;
  height: 100%;
  background-color: white;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
}

.jackpot {
  font-size: 20px;
  font-weight: bold;
}

.bar {
  background-color: black;
  color: white;
  font-weight: bold;
  font-size: 30px;
  padding: 0 5px;
}

.yellow {
  color: yellow;
  text-shadow: 2px 2px 0 #000000, 2px -2px 0 #000000, -2px 2px 0 #000000, -2px -2px 0 #000000, 2px 0px 0 #000000, 0px 2px 0 #000000, -2px 0px 0 #000000, 0px -2px 0 #000000, 2px 2px 2px rgba(0, 0, 0, 0);
}

.gold {
  color: #caa741;
  text-shadow: 2px 2px 0 #000000, 2px -2px 0 #000000, -2px 2px 0 #000000, -2px -2px 0 #000000, 2px 0px 0 #000000, 0px 2px 0 #000000, -2px 0px 0 #000000, 0px -2px 0 #000000, 2px 2px 2px rgba(0, 0, 0, 0);
}

.purple {
  color: purple;
  text-shadow: 2px 2px 0 #000000, 2px -2px 0 #000000, -2px 2px 0 #000000, -2px -2px 0 #000000, 2px 0px 0 #000000, 0px 2px 0 #000000, -2px 0px 0 #000000, 0px -2px 0 #000000, 2px 2px 2px rgba(0, 0, 0, 0);
}

.red {
  color: red;
  text-shadow: 2px 2px 0 #000000, 2px -2px 0 #000000, -2px 2px 0 #000000, -2px -2px 0 #000000, 2px 0px 0 #000000, 0px 2px 0 #000000, -2px 0px 0 #000000, 0px -2px 0 #000000, 2px 2px 2px rgba(0, 0, 0, 0);
}

.green {
  color: green;
  text-shadow: 2px 2px 0 #000000, 2px -2px 0 #000000, -2px 2px 0 #000000, -2px -2px 0 #000000, 2px 0px 0 #000000, 0px 2px 0 #000000, -2px 0px 0 #000000, 0px -2px 0 #000000, 2px 2px 2px rgba(0, 0, 0, 0);
}

.minitic, .microtic {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 100%;
  margin: 0 auto;
}
.microtic {
  font-size: 12px;

}

.miniticContainer, .microticContainer {
  display: inline-block;
  width: 120px;
  max-width: 120px;
  overflow: hidden;
  text-align: center;
  margin: 5px auto;
}

.microticContainer {
  width: 75px;
  max-width: 75px;
}
.miniticgrid, .microticgrid  {
  display: grid;
  grid-template-columns: repeat(3, 25px);
  grid-template-rows: repeat(3, 25px);
  color: black;
  border: none;
  max-width: 77px;
  background-color: black;
  grid-gap: 1px;
  margin: 0 auto;
  margin-bottom: 3px;
}
.microticgrid  {
  grid-template-columns: repeat(3, 15px);
  grid-template-rows: repeat(3, 15px);
    max-width: 47px;

}

.miniticgrid div, .microticgrid div  {
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}
.opponentname {
  font-size: 14px;
}
#lobby, #gamechat {
  position: absolute;
  z-index: 254;
  right: 5px;
  bottom: 5px;
  top: 45px;
  height: calc(100vh - 50px);
  width: calc(30vw - 20px);
  max-width: 40%;
  background-color: white;
  color: black;
  border: 1px solid gray;
  overflow: hidden;
}

#gamechat {
  right: calc(30vw - 10px);
  bottom: 5px;
  top: auto;
  left: 5px;
  height: 200px;
  width: calc(70vw - 5px);
  max-width: calc(70vw - 5px);
}

#lobbyheader, #gamechatheader {
  height: 20px;
  max-height: 20px;
  overflow: hidden;
}

.chatlist {
  overflow-y: scroll;
  overflow-x: hidden;
  padding-right: 8px;
}

.lobbychatlist {
  height: 442px;
  height: calc(100vh - 108px);
  background-color: #eff4f7;
}

.gamechatlist {
  height: 142px;
}

.chatlist::-webkit-scrollbar {
  width: 10px;
}

.chatlist::-webkit-scrollbar-track {
  background: #f1f1f1;
}

.chatlist::-webkit-scrollbar-thumb {
  background: #040e23;
}

.chatlist::-webkit-scrollbar-thumb:hover {
  background: rgba(57, 87, 150, 0.35);
}

#lobbyheader, #gamechatheader {
  background-color: #040e23;
  color: white;
  padding: 5px;
}

#gamechatheader {
  background-color: #0c3a5e;
}

#lobbyform, #gamechatform {
  background-color: #040e23;
  position: absolute;
  bottom: 0px;
  width: 100%;
  height: 28px;
  max-height: 28px;
  overflow: hidden;
}

#gamechatform {
  background-color: #0c3a5e;
}

#lobbyform input, #gamechatform input {
  width: 310px;
  border-radius: 3px;
  padding: 3px 5px;
  border: none;
}

#gamechatform input {
  width: 510px;
}

#lobbyform input {
  background-color: #eff4f7;
}

#lobbyform button, #gamechatform button {
  background-color: #040e23;
  color: white;
  border: none;
  padding: 3px 5px;
}

#gamechatform button {
  background-color: #0c3a5e;
}

.box {
  border: 1px solid gray;
  padding: 5px 8px;
  width: 40vw;
  background-color: white;
  position: relative;
  margin: 0 auto;
  margin-bottom: 5px;
  overflow: hidden;
}

.jscolor {
  width: 20px;
  height: 20px;
  color: transparent !important;
}

.chat {
  border-bottom: 1px solid gray;
  padding: 5px;
  width: 100%;
}

header ul {
  display: flex;
  justify-content: space-around;
  list-style: none;
}

.card {
  background-color: white;
  border: 1px solid black;
  padding: 5px;
  border-radius: 5px;
  height: 150px;
  width: 100px;
}

.club, .spade {
  color: black;
}

.heart, .diamond {
  color: red;
}

.cardsuits ul {
  list-style: none;
  color: black;
}

.cardsuits ul li:nth-child(4n-3):before {
  content: '\2660 \0020';
  color: black;
}

.cardsuits ul li:nth-child(4n-2):before {
  content: '\2665 \0020';
  color: red;
}

.cardsuits ul li:nth-child(4n-1):before {
  content: '\2663 \0020';
  color: black;
}

.cardsuits ul li:nth-child(4n-0):before {
  content: '\2666 \0020';
  color: red;
}

.hello input {
  display: inline;
  background-color: transparent;
  border: none;
  font-size: 16px;
  font-weight: bold;
  margin-left: -5px;
}

.box a {
  color: black;
}

#tictactoe {
  text-align: center;
  width: 310px;
  background-color: black;
  border: none;
  padding: 0;

}
#ticinfo {
  text-align: center;
  width: 310px;
  margin: 0 auto;
}
.yourturn {
  background-color: rgb(255, 246, 164) !important;
}
.bg {
  padding: 5px;
  background-color: white;
}

#tictactoegrid {
  display: grid;
  grid-template-columns: repeat(3, 100px);
  grid-template-rows: repeat(3, 100px);
  grid-gap: 5px;
  width: 500px;
  margin: 0 auto;

}

.tic {
  background-color: white;
  border: none;
  width: 100px;
  height: 100px;
  max-width: 100px;
  max-height: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 50px;
}

.tic button {
  border: none;
  background: transparent;
  width: 100px;
  height: 100px;
  font-size: 20px;
}

.tic button:hover {
  background-color: rgb(178, 233, 129);
}

.winschart {
  display: grid;
  grid-template-columns: repeat(2, 50%);
  background-color: #f0ecd4;
  padding: 3px;
  width: 300px;
}

.whoisonline {
  float: right;
  border: 1px solid black;
  width: 150px;
  padding: 5px;
}

#onlineusers {}

canvas {
  cursor: crosshair;
}

.px2 {
  cursor: crosshair;
}

.px5 {
  cursor: url('<%= image_path('px5.png') %>') 4 4, crosshair;
}

.px10 {
  cursor: url('<%= image_path('px10.png') %>') 5 5, crosshair;
}

.px20 {
  cursor: url('<%= image_path('px20.png') %>') 10 10, crosshair;
}

.px50 {
  cursor: url('<%= image_path('px50.png') %>') 25 25, crosshair;
}

.px100 {
  cursor: url('<%= image_path('px100.png') %>') 50 50, crosshair;
}

@media only screen and (max-width: 800px) {
  header {
    height: 40px;
    max-height: 40px;
    -webkit-transition: height .25s;
    transition: height .25s;
  }

  header:hover {
    height: 180px;
    max-height: 180px;
  }

  header ul {
    display: block;
    text-align: center;
  }
}

.pictionarycolorpicker {
  width: 300px;
}

.pictcolors {
  height: 30px;
  width: 30px;
  border: 1px solid gray;
  border-radius: 3px;
  margin: 4px 2px;
}

.pictbrush {
  height: 30px;
  width: 36px;
  background-color: white;
  border: 1px solid gray;
  border-radius: 3px;
  margin: 4px 2px;
}

@media only screen and (max-width: 600px) {
  body {
    font-size: 14px;
  }

  header {
    height: 35px;
    max-height: 35px;
  }

  header:hover {
    height: 150px;
    max-height: 150px;
  }

  .content {
    padding: 0;
    margin-left: 0;
    margin-right: 0;
  }

  .box {
    border-left: none;
    border-right: none;
    width: 100vw;
  }

  #slotmachine button {
    width: 90vw;
    max-width: 90vw;
  }

  #slotreel, #slotflash {
    width: 90vw;
    max-width: 90vw;
  }

  #lobby, #gamechat {
    position: fixed;
    bottom: 0;
    top: auto;
    left: 0;
    right: 0;
    height: 20vh;
    max-height: 20vh;
    width: 100vw;
    max-width: 100vw;
  }

  #gamechat {
    bottom: 20vh;
    top: auto;
  }

  #lobbyform input, #gamechatform input {
    width: calc(100vw - 77px);
    max-width: calc(100vw - 77px);
  }

  #lobbyform button, #gamechatform button {
    width: 50px;
    max-width: 50px;
    padding: 0;
  }

  .lobbychatlist {
    height: calc(20vh - 58px);
  }

  .gamechatlist {
    height: calc(20vh - 58px);
  }
}

@media only screen and (max-height: 800px) {
  #tictactoe, #ticinfo {
    width: 250px;
  }

  #tictactoegrid {
    grid-template-columns: repeat(3, 80px);
    grid-template-rows: repeat(3, 80px);
    grid-gap: 5px;
    width: 500px;
  }

  .tic {
    width: 80px;
    height: 80px;
    max-width: 80px;
    max-height: 80px;
    font-size: 50px;
  }

  .tic button {
    width: 80px;
    height: 80px;
    font-size: 20px;
  }

  .winschart {
    width: 245px;
  }
}
